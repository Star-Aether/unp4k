<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:pages="clr-namespace:unp4k.Pages"
            x:Class="unp4k.MainPage"
            Title="unp4k"
            IconImageSource="dotnet_bot.svg">
    
    <NavigationPage Title="Home" IconImageSource="{StaticResource TabHome}">
        <x:Arguments>
            <pages:HomePage Title="Home" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Browser" IconImageSource="{StaticResource TabBrowser}">
        <x:Arguments>
            <pages:BrowserPage Title="Browser" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Extraction" IconImageSource="{StaticResource TabExtraction}">
        <x:Arguments>
            <pages:ExtractionPage Title="Extraction" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Terminal" IconImageSource="{StaticResource TabTerminal}">
        <x:Arguments>
            <pages:TerminalPage Title="Terminal" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="About" IconImageSource="{StaticResource TabAbout}">
        <x:Arguments>
            <pages:AboutPage Title="About" />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
